{
  "name": "borodutch-stats",
  "version": "1.0.0",
  "description": "Borodutch stats server",
  "main": "dist/app.js",
  "repository": "https://github.com/backmeupplz/borodutch-stats",
  "author": "backmeupplz <backmeupplz@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "distribute": "yarn build-ts && node dist/app.js",
    "develop": "concurrently -i -k -p \"[{name}]\" -n \"Node, TypeScript\" -c \"yellow.bold,cyan.bold\" \"yarn watch-js\" \"yarn watch-ts\"",
    "build-ts": "tsc --skipLibCheck",
    "watch-ts": "tsc -w --skipLibCheck",
    "watch-js": "nodemon dist/app.js"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.18.11"
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^6.1.1",
    "@types/mongoose": "^5.5.12",
    "@types/node": "^12.7.2",
    "axios": "^0.21.1",
    "dotenv": "^8.0.0",
    "koa": "^2.7.0",
    "koa-bodyparser-ts": "^0.3.1",
    "koa-router-ts": "^3.0.0",
    "mongoose": "^5.6.9",
    "telegraf": "^3.37.0",
    "typegoose": "^5.9.0",
    "typescript": "^3.5.3"
  }
}
